# ADD LOCAL CONFIGURATION HERE
[alias]
  mylog = log --author=apraditya
  co    = checkout

# DO NOT EDIT BELOW THIS LINE
[push]
  default = tracking
[color]
	diff = auto
	status = auto
	branch = auto
[core]
	excludesfile = /Users/apraditya/.gitignore_global
[format]
  pretty = %Cblue%h%Creset %Cgreen[%cr]%Creset (%an) %s
[alias]
  c = commit -m
  a = add
  di = diff
  dic = diff --cached
  pl = pull
  ps = push
  plre = pull --rebase
  st = status
  out = log origin..HEAD
  qpl = log --pretty=\"%Cblue%h%Creset %Cgreen[%cr]%Creset %s\" origin/production..origin/master
  graph = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
  ci = commit
  ca = commit --amend
  ribbon = tag --force _ribbon origin/master
  catchup = log --patch --reverse --topo-order _ribbon..origin/master
[user]
	name = Adinda Praditya
	email = apraditya@gmail.com
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path =
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.localized/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
